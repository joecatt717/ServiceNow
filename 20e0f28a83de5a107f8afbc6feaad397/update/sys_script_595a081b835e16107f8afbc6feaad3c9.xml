        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

	// check if record is unique - abort if false
	var coalesce_fields = ['table','record', 'attribute_extension_type', 'attribute_extension_rule'];

	var urit = new x_1579565_cmdb_a_1.UniqueRecordIdentificationTool();
	var is_unique_record = urit.validateUniqueRecord(current, coalesce_fields);

	if (!is_unique_record) {
		gs.addErrorMessage(urit.error_code);
		current.setAbortAction(true);
	}

})(current, previous);]]></script>
